{
    "api_version": "v1",
    "endpoints": {
        "users": {
            "get": {
                "description": "获取用户信息",
                "parameters": {
                    "id": "string",
                    "fields": "array[string]"
                },
                "responses": {
                    "200": "成功",
                    "404": "用户未找到"
                }
            },
            "post": {
                "description": "创建新用户",
                "required_fields": [
                    "username",
                    "email",
                    "password"
                ]
            }
        }
    }
}