            "rpm": 20,
            "maxTokensPerSegment": 1500,
            "timeout": 30,
            "temperature": 0.0,
            "streamMode": true
        },
        {
            "name": "deepseek", // 需付费
            "apiEndpoint": "https://api.deepseek.com/v1",
            "apiKeyEnvVarName": "DEEPSEEK_API_KEY",
