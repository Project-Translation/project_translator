{
    "projectTranslator.currentVendor": "siliconflow",
    "projectTranslator.vendors": [
        {
            "name": "gemini",
            "apiEndpoint": "https://generativelanguage.googleapis.com/v1beta",
            "apiKeyEnvVarName": "GEMINI_API_KEY",
            "model": "gemini-2.0-flash",
            "rpm": 15,
            "maxTokensPerSegment": 1000,
            "timeout": 30,
            "streamMode": true
        },
        {
            "name": "grok",
            "apiEndpoint": "https://api.x.ai/v1",
            "apiKeyEnvVarName": "GROK_API_KEY",
            "model": "grok-3-mini",
            "rpm": 20,
            "maxTokensPerSegment": 1500,
            "timeout": 30,
            "temperature": 0.0,
            "streamMode": true
        },
        {
            "name": "deepseek",
            "apiEndpoint": "https://api.deepseek.com/v1",
            "apiKeyEnvVarName": "DEEPSEEK_API_KEY",
            "model": "deepseek-chat", // deepseek-chat, deepseek-reasoner
            "rpm": 0,
            "maxTokensPerSegment": 4000,
            "timeout": 30,
            "streamMode": true
        },
        {
            "name": "aliyun",
            "apiEndpoint": "https://dashscope.aliyuncs.com/compatible-mode/v1",
            "apiKeyEnvVarName": "QWEN_API_KEY",
            "model": "qwen3-30b-a3b", // https://bailian.console.aliyun.com/?productCode=p_efm#/model-market/detail/qwq-32b
            "rpm": 0,
            "maxTokensPerSegment": 400000,
            "timeout": 120,
            "streamMode": true
        },
        {
            "name": "siliconflow",
            "apiEndpoint": "https://api.siliconflow.cn/v1",
            "apiKeyEnvVarName": "SILICONFLOW_API_KEY",
            "model": "Qwen/Qwen3-8B",
            "rpm": 600,
            "maxTokensPerSegment": 3000,
            "timeout": 30,
            "streamMode": true
        },
        {
            "name": "openrouter",
            "apiEndpoint": "https://openrouter.ai/api/v1",
            "apiKeyEnvVarName": "OPENROUTER_API_KEY",
            "model": "qwen/qwen3-14b:free",
            "rpm": 0,
            "maxTokensPerSegment": 10000,
            "timeout": 30,
            "streamMode": true
        }
    ],
    "projectTranslator.translationIntervalDays": 29,
    "projectTranslator.userPrompts": [
        "1. Should return no need translate if the markdown file has 'draft' set to 'true' in the front matter.",
        "2. './readmes/' in the sentences should replace with './'",
    ],
    "projectTranslator.ignore": {
        "paths": [
            "embeded/zh-cn/**"
        ],
        "extensions": [
            ".log"
        ]
    },
    "projectTranslator.specifiedFolders": [
        {
            "sourceFolder": {
                "path": "embeded",
                "lang": "en-us"
            },
            "targetFolders": [
                {
                    "path": "embeded/zh-cn",
                    "lang": "zh-cn"
                }
            ]
        },
        {
            "sourceFolder": {
                "path": "structure/json",
                "lang": "en-us"
            },
            "targetFolders": [
                {
                    "path": "i18n/zh-cn/structure/json",
                    "lang": "zh-cn"
                }
            ]
        }
    ],
    "projectTranslator.specifiedFiles": [
