{
    "projectTranslator.debug": true,
    "projectTranslator.currentVendor": "modelscope",
    "projectTranslator.vendors": [
        {
            "name": "modelscope",
            "apiEndpoint": "https://api-inference.modelscope.cn/v1/",
            "apiKeyEnvVarName": "MODELSCOPE_SDK_TOKEN",
            "model": "Qwen/Qwen3-235B-A22B",
            "rpm": 0,
            "maxTokensPerSegment": 4000,
            "timeout": 30,
            "streamMode": true
        }
    ],
    "projectTranslator.translationIntervalDays": 29,
    "projectTranslator.userPrompts": [
        "1. Should return no need translate if the markdown file has 'draft' set to 'true' in the front matter.",
        "2. './readmes/' in the sentences should replace with './'"
    ],
    "projectTranslator.ignore": {
        "paths": [
            "embeded/zh-cn/**"
        ],
        "extensions": [
            ".log"
        ]
    },
    "projectTranslator.specifiedFolders": [
        {
            "sourceFolder": {
                "path": "embeded",
                "lang": "en-us"
            },
            "targetFolders": [
                {
                    "path": "embeded/zh-cn",
                    "lang": "zh-cn"
                }
            ]
        },
        {
            "sourceFolder": {
                "path": "structure/json",
                "lang": "en-us"
            },
            "targetFolders": [
                {
                    "path": "i18n/zh-cn/structure/json",
                    "lang": "zh-cn"
                }
            ]
        }
    ],
    "projectTranslator.specifiedFiles": [
        // should not translate
        {
            "sourceFile": {
                "path": "structure/md/should_skip.md",
                "lang": "en-us"
            },
            "targetFiles": [
                {
                    "path": "i18n/md/should_skip.zh-cn.md",
                    "lang": "zh-cn"
                },
                {
                    "path": "i18n/md/should_skip.zh-hk.md",
                    "lang": "zh-hk"
                }
            ]
        },
        // {
        //     "sourceFile": {
        //         "path": "structure/md/should_translate.md",
        //         "lang": "en-us"
        //     },
