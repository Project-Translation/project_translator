{
    "api_version": "v1",
    "endpoints": {
        "users": {
            "get": {
                "description": "Retrieve user information",
                "parameters": {
                    "id": "string",
                    "fields": "array[string]"
                },
                "responses": {
                    "200": "Success",
                    "404": "User not found"
                }
            },
            "post": {
                "description": "Create new user",
                "required_fields": [
                    "username",
                    "email",
                    "password"
                ]
            }
        }
    }
}